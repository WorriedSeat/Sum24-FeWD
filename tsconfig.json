{
    "compilerOptions": {
      "target": "es5",
      "module": "esnext",
      "jsx": "react",
      "sourceMap": true,
      "outDir": "dist",
      "strict": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "allowImportingTsExtensions": true
    },
    "scripts": {
    "build": "./src/index.tsx"
    }
  }
  
